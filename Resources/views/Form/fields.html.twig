{% block genemu_routing_widget %}
{{ form_widget(form) }}

<script type="text/javascript">
    jQuery('#{{ form.patterns.get('id') }}').genemuCollection({
        i18n: {
            add: '{% trans %}Add Pattern{% endtrans %}',
            confirm: '{% trans %}Are you sure ?{% endtrans %}',
            remove: '{% trans %}Remove{% endtrans %}'
        }
    });
    jQuery('#{{ form.routingparameters.get('id') }}').genemuCollection({
        i18n: {
            add: '{% trans %}Add Parameter{% endtrans %}',
            confirm: '{% trans %}Are you sure ?{% endtrans %}',
            remove: '{% trans %}Remove{% endtrans %}'
        }
    });
</script>
{% endblock genemu_routing_widget %}
