<!DOCTYPE html>
<html{% if culture is defined %} lang="{{ culture }}" xml:lang="{{ culture }}"{% endif %}>
    <head>
        <title>{% trans %}Routing Manager{% endtrans %}</title>
        <meta name="title" content="{% trans %}Routing Manager{% endtrans %}" />
        <meta content="text/html; charset=utf-8" http-equiv="content-type" />
        <link href="favicon.ico" type="image/x-icon" rel="shortcut icon" />
        {% if culture is defined %}
        <meta name="language" content="{{ culture }}" />
        <meta content="{{ culture }}" http-equiv="content-lenguage" />
        {% endif %}
        {% block links "" %}
        {% block scripts "" %}
    </head>
    <body>
        <header id="header">
            <h1>{% trans %}Routing Manager{% endtrans %}</h1>
        </header>
        <div id="content">
            {% block body "" %}
        </div>
    </body>
</html>
